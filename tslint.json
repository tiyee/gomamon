{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
 
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "file-name-casing": [false, "camel-case"],
    "object-literal-sort-keys": false,
    "no-unnecessary-generics": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  }
}
